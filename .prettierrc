{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "strict",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "auto",
    "rangeStart": 0,
    "vueAttributeLineBreak": {
        "singleline": "never",
        "multiline": {
            "start": "always",
            "end": "always"
        }
    },
    "singleAttributePerLine": true
}
